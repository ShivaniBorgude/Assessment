#include <iostream>
#include <string>
using namespace std;

class Car {
  public:
  string brand;
  string model;
  int year;
};

int main() {
  //Create an object of Car
  Car carObj1;
  carObj1.brand ="Audi";
  carObj1.model ="RS5";
  carObj1.year =2010;
  
  //Create an object of Car
  Car carObj2;
  carObj2.brand ="Hyundai";
  carObj2.model ="Venue";
  carObj2.year =2024;
  
  //Create an object of Car
  Car carObj3;
  carObj3.brand ="Ford";
  carObj3.model ="Ford Aspire";
  carObj3.year =1993;
  
  //Print atribute values
  cout<<carObj1.brand<<" "<<carObj1.model<<" "<<carObj1.year<<"\n";
  cout<<carObj2.brand<<" "<<carObj2.model<<" "<<carObj3.year<<"\n";
  cout<<carObj3.brand<<" "<<carObj3.model<<" "<<carObj3.year<<"\n";
  
  return 0; 
}